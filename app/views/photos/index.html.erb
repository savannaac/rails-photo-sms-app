<h1><%= image_tag("boston-terrier-logo.jpg", height: "130", width: "130") %></h1>
<h2><%= current_user.name %>'s photos</h2>

<%= link_to "home", root_path %><br />
<%= link_to "create new photo", new_user_photo_path(current_user) %><br /><br>
<%= button_to "sign out", destroy_user_session_path, method: "delete"%><br><br><br>

<% @photos.each do |photo| %>
    <%= link_to(image_tag(photo.img_url, height: 200, width: 200), user_photo_path(current_user, photo)) %>
<% end %>